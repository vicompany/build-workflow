{
  "scripts": {
    "build": "gulp & npm run browserify:build & npm run rollup:build & npm run webpack:build",
    "browserify:build": "browserify -g uglifyify Source/scripts/app-browserify.js -o wwwroot/bundle-browserify-npm.js",
    "rollup:build": "rollup -c",
    "rollup:watch": "npm run rollup:build -- --watch",
    "webpack:build": "webpack --config webpack.config.js",
    "webpack:watch": "npm run webpack:build -- --watch"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "latest"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-loader": "^6.2.9",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.0.0",
    "merge-stream": "^1.0.1",
    "node-sass": "^4.5.0",
    "ramda": "^0.22.1",
    "rollup": "^0.37.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-stream": "^1.19.0",
    "rollup-watch": "^2.5.0",
    "sass-loader": "^4.1.1",
    "uglifyify": "^3.0.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "moment": "^2.17.1"
  }
}
